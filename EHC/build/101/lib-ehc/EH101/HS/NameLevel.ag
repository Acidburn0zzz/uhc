    | KindSignature
        loc             .   nmLev       =   @lhs.nmLev + 1
    | Class InstanceUseImplicitly Default
        loc             .   nmLev       =   @lhs.nmLev -- + 1
    | Instance
        loc             .   nmLev       =   @lhs.nmLev -- + 1
        -- where           .   nmLev       =   @lhs.nmLev + 1
    | Type
        loc             .   nmLev       =   @lhs.nmLev -- + 1

SEM Type
